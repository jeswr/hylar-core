{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-restricted-syntax": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "guard-for-in":"off",
        "no-param-reassign": "off",
        "no-use-before-define": "off"
    }
}
