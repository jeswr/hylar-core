{
  "name": "hylar-core",
  "version": "1.0.0",
  "description": "A lightweight module containing core reasoner logic from HyLAR (https://github.com/ucbl/HyLAR-Reasoner.git)",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "OWL",
    "Reasoner",
    "HyLAR"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/n3": "^1.10.3",
    "@types/rdf-js": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.5",
    "n3": "^1.11.1",
    "pre-commit": "^1.2.2",
    "rdf-js": "^4.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".d.ts"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 35,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    }
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.3.4",
    "md5": "^2.3.0",
    "rdf-string": "^1.6.0",
    "rdf-string-ttl": "^1.2.0"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main"
    ]
  }
}
